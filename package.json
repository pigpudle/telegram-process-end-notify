{
  "name": "telegram-process-end-notify",
  "version": "1.0.0",
  "description": "This package allows you to watch your local processes and get notifications in Telegram when a specific process finishes.",
  "keywords": [
    "telegram",
    "node",
    "process"
  ],
  "license": "MIT",
  "main": "./src/index.js",
  "scripts": {
    "start": "node ./src/index",
    "start:2": "node ./src/main"
  },
  "bin": {
    "tg-notify": "npm run start:2"
  },
  "author": "Neli Harbuzava",
  "dependencies": {
    "dotenv": "10.0.0",
    "node": "14.8.0",
    "process-end-notify": "2.0.3",
    "telegraf": "4.3.0",
    "telegraf-inline-menu": "6.2.1"
  }
}
